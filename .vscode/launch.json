            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/ArtStationDownloader.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: ArtStationDownload in Terminal",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/ArtStationDownloader.py",
            "args": [
                "-u",
                "ase",
                "ikaired",
                "-d",
                "${env:UserProfile}\\Desktop\\ArtStation",
                "-t",
                "all"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
